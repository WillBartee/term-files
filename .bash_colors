
# Setup color functions

function BLACK() {
  echo -ne "\e[0;30m"
}
function DGRAY() {
  echo -ne "\e[90m"
}
function GRAY() {
  echo -ne "\e[30;1m"
}
function LGRAY() {
  echo -ne "\e[0;37m"
}
function GRAYHL() {
  echo -ne "\e[100m"
}
function RED() {
  echo -ne "\e[0;31m"
}
function LRED() {
  echo -ne "\e[1;31m"
}
function GREEN() {
  echo -ne "\e[0;32m"
}
function LGREEN() {
  echo -ne "\e[1;32m"
}
function BROWN() {
  echo -ne "\e[0;33m"
}
function YELLOW() {
  echo -ne "\e[1;33m"
}
function LYELL() {
  echo -ne "\e[93m"
}
function BLUE() {
  echo -ne "\e[0;34m"
}
function LBLUE() {
  echo -ne "\e[1;34m"
}
function BLUEHL() {
  echo -ne "\e[44m"
}
function PURPLE() {
  echo -ne "\e[0;35m"
}
function LPURPLE() {
  echo -ne "\e[1;35m"
}
function CYAN() {
  echo -ne "\e[0;36m"
}
function LCYAN() {
  echo -ne "\e[1;36m"
}
function WHITE() {
  echo -ne "\e[1;37m"
}
function NEUTRAL() {
  echo -ne "\e[0m"
}
function NONE() {
  echo -ne "\e[0m"
}
function DIM() {
  echo -ne "\e[2m"
}

function printcolors() {
  for clbg in {40..47} {100..107} 49 ; do
    #Foreground
    for clfg in {30..37} {90..97} 39 ; do
      #Formatting
      for attr in 0 1 2 4 5 7 ; do
        #Print the result
        echo -en "\e[${attr};${clbg};${clfg}m ^[${attr};${clbg};${clfg}m \e[0m"
      done
      echo #Newline
    done
  done
}
